    <link href='http://fonts.googleapis.com/css?family=Kaushan+Script' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet'
          type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>

    <!--angular loading bar css-->
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/angular-loading-bar/0.6.0/loading-bar.min.css"/>

    <!--toastr-->
    <link rel="stylesheet" href="/bower_components/toastr/toastr.min.css">

    <!-- loads custom css files -->
    <link rel="stylesheet" href="/public/css/cards.css">
    <link rel="stylesheet" href="/public/css/main.css">
    <link rel="stylesheet" href="/public/css/client/client_home.css">

    <!--google analytics-->
    <script>
        //trying to exclude localhost
        if (document.location.hostname.search("harvardgrill") !== -1) {
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r;
                i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date();
                a = s.createElement(o),
                        m = s.getElementsByTagName(o)[0];
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m)
            })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

            ga('create', 'UA-58209445-2', 'auto');
            ga('require', 'linkid', 'linkid.js');
            <%- gAnalyticsUserId %>
            ga('send', 'pageview');
        }
    </script>


    <title>HarvardGrill - Login</title>
</head>

<body>

<!--toast notifications-->
<toast></toast>
<!--end of toast-->


<!--start of nav-->
<div client-home-top-nav></div>
<!--end of nav-->

<!--start of complete registration form-->
<div ng-if="!isFullyRegistered">
    <div complete-registration></div>
</div>
<!--end of complete registration form-->

<!--start of client info login form-->
<div ng-if="isFullyRegistered">
    <div client-info-login></div>
</div>
<!--end of client info login form-->


<!-- JAVASCRIPT -->

